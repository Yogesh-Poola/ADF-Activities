{
	"name": "FileParameterDS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"InititalName": {
				"type": "string",
				"defaultValue": "Default"
			},
			"NumberOfPeople": {
				"type": "int",
				"defaultValue": "Default"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().InititalName,'-',string(dataset().NumberOfPeople),'.csv')",
					"type": "Expression"
				},
				"folderPath": "input",
				"fileSystem": "source"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "enrollee_id",
