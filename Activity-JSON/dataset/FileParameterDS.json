				"type": "String"
			},
			{
				"name": "gender",
				"type": "String"
			},
			{
				"name": "relevent_experience",
				"type": "String"
			},
			{
				"name": "enrolled_university",
				"type": "String"
			},
			{
				"name": "education_level",
				"type": "String"
			},
			{
				"name": "major_discipline",
				"type": "String"
			},
			{
				"name": "experience",
				"type": "String"
			},
			{
				"name": "company_size",
				"type": "String"
			},
			{
				"name": "company_type",
				"type": "String"
			},
			{
				"name": "training_hours",
				"type": "String"
			},
			{
				"name": "target",
				"type": "String"
			}
		]
	}
}